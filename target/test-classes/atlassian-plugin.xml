<?xml version="1.0" encoding="UTF-8"?>

<atlassian-plugin key="ro.etss.jira.plugin.tutorial.JiraPluginTutorial-tests" name="JiraPluginTutorial" plugins-version="2">
  <plugin-info>
    <description>This is the ro.etss.jira.plugin.tutorial:JiraPluginTutorial plugin for Atlassian JIRA.</description>
    <version>1.0</version>
    <vendor name="ETSS" url="http://www.euro-testing.com/"/>
  </plugin-info>
  <!-- from our base plugin -->
  <component-import key="myComponent" interface="ro.etss.jira.plugin.tutorial.main.api.MyPluginComponent"/>
  <!-- from the product container -->
  <component-import key="applicationProperties" interface="com.atlassian.sal.api.ApplicationProperties"/>
  <component key="my-component-wired-test" class="it.ro.etss.jira.plugin.tutorial.main.MyComponentWiredTest" public="true">
    <interface>it.ro.etss.jira.plugin.tutorial.main.MyComponentWiredTest</interface>
    <service-properties>
      <entry key="inProductTest" value="true"/>
    </service-properties>
  </component>
</atlassian-plugin>
